                    <div class="card-body p-0">
                        <div class="table-responsive">
                            <table class="table table-hover mb-0">
                                <thead>
                                    <tr>
                                        <th>
                                            <div class="chk-option">
                                                <label class="check-task custom-control custom-checkbox d-flex justify-content-center done-task">
                                                    <input type="checkbox" class="custom-control-input" checked>
                                                    <span class="custom-control-label"></span>
                                                </label>
                                            </div>
                                            Employee Assigned
                                        </th>
                                        <th>Project Name</th>
                                        <th>Due Date</th>
                                        <th class="text-right">Priority</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Projects)
                                    {
                                        <tr>
                                            <td>
                                                <div class="chk-option">
                                                    <label class="check-task custom-control custom-checkbox d-flex justify-content-center done-task">
                                                        <input type="checkbox" class="custom-control-input" checked>
                                                        <span class="custom-control-label"></span>
                                                    </label>
                                                </div>
                                        <div class="d-inline-block align-middle">
                                                    <img src="~/media/profiles/@item.User.Image" alt="user image" class="img-radius wid-40 align-top m-r-15">
                                                    <div class="d-inline-block">
                                                        <h6>@item.User.Name</h6>
                                                        @{
                                                            var currentRoleNames = await userManager.GetRolesAsync(await userManager.FindByIdAsync(item.User.Id));
                                                        }
                                                        <p class="text-muted m-b-0">@currentRoleNames[0]</p>
                                                    </div>
                                                </div>
                                            </td>
                                            <td>@item.Name</td>
                                            <td>@item.DueDate</td>                                          
                                            
                                            <td class="text-right">
                                                 @if (item.Priority == 0)
                                                {
                                                    <label class="badge badge-light-danger">Low</label>
                                                }
                                                @if (item.Priority == 1)
                                                {
                                                    <label class="badge badge-light-primary">Medium</label>
                                                }
                                                @if (item.Priority == 2)
                                                {
                                                    <label class="badge badge-light-success">High</label>
                                                }
                                            </td>
                                        </tr>    
                                    }                                                                                    
                                </tbody>
                            </table>
