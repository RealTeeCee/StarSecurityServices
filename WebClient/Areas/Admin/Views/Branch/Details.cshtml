@model Models.Branch

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
    
    <div class="container">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="Name">NO</label>
                <input type="text" class="form-control" asp-for="@Model.Id" readonly>
            </div>

            <div class="form-group col-md-6">
                <label for="Email">Email</label>
                <input type="email" class="form-control" asp-for="@Model.Email" readonly>
            </div>         
        </div>

        <div class="form-row">      
            <div class="form-group col-md-6">
                <label for="Youtube">Youtube</label>
                <input type="text" class="form-control" asp-for="@Model.Youtube" readonly>
            </div>            
            <div class="form-group col-md-6">
                <label for="Phone">Phone</label>
                <input type="text" class="form-control" asp-for="@Model.Phone" readonly>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="Facebook">Facebook</label>
                <input type="text" class="form-control" asp-for="@Model.Facebook" readonly>
            </div>
            <div class="form-group col-md-6">
                <label for="TimeOpen">Time Open</label>
                <input type="text" class="form-control" asp-for="@Model.TimeOpen" readonly>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="Twitter">Twitter</label>
                <input type="text" class="form-control" asp-for="@Model.Twitter" readonly>
            </div>
            <div class="form-group col-md-6">
                <label for="Name">Name</label>
                <input type="text" class="form-control" asp-for="@Model.Name" readonly>
            </div>
           
        </div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="Instagram">Instagram</label>
                <input type="text" class="form-control" asp-for="@Model.Instagram" readonly>
            </div>
     
            <div class="form-group col-md-6">
                <label for="CreatedAt">Created At </label>
                <input type="text" class="form-control" asp-for="@Model.CreatedAt" readonly>
            </div>
        </div>
         <div class="form-row">
            <div class="form-group col-md-12">
                <label for="Address">Address</label>
                <input type="text" class="form-control" asp-for="@Model.Address" readonly>
            </div>
         </div>

        <div class="form-row d-flex justify-content-center">
                <a asp-action="Index" class="btn btn-primary">Back to List</a>
            </div>
        </div>



@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }