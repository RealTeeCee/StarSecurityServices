@model Models.ClientDetail

@{
    ViewData["Title"] = "Details";
}

<div class="container">
    <div class="form-row">
          <div class="form-group col-md-6">
            <label for="Name">Name</label>
            <input type="text" class="form-control" asp-for="Name" readonly>
        </div>
        <div class="form-group col-md-6">
            <label for="Email">Email</label>
            <input type="text" class="form-control" asp-for="Email" readonly>
        </div>      
    </div>

    <div class="form-row">
        <div class="form-group col-md-12">
            <label for="Title">Title </label>
            <input type="text" class="form-control" asp-for="Address" readonly>
        </div>        
    </div>

    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="CreatedAt">Created At</label>
            <input type="text" class="form-control" asp-for="CreatedAt" readonly>
        </div>
        <div class="form-group col-md-6">
            <label for="UpdatedAt">Updated At</label>
            <input type="text" class="form-control" asp-for="UpdatedAt" readonly>
        </div>
    </div>


    <div class="form-row d-flex justify-content-center">
        <a asp-action="Index" class="btn btn-primary">Back to List</a>
    </div>
</div>